<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <VersionPrefix>6.0.4</VersionPrefix>
    <TargetFramework>net6.0</TargetFramework>
    <AssemblyName>Pomelo.EntityFrameworkCore.Lolita</AssemblyName>
    <PackageId>Pomelo.EntityFrameworkCore.Lolita</PackageId>
    <PackageTags>Entity Framework Core;Extensions;Bulk Update;Bulk Delete</PackageTags>
    <PackageIconUrl>https://avatars.githubusercontent.com/u/118060120</PackageIconUrl>
    <PackageProjectUrl>https://github.com/PomeloFoundation/Lolita</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/PomeloFoundation/Lolita/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>git://github.com/pomelofoundation/lolita</RepositoryUrl>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="6.0.0" />
  </ItemGroup>

</Project>
